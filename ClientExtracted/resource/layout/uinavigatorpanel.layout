		}
		
		
		grouper
		{
			bgcolor=none
			render_bg
			{
				// background fill
				0="fill( x0 + 1, y0 + 1, x0 + 276, y1 - 1, dialogbg )"
	      1="fill( x0 + 279, y0 + 1, x1 - 10, y1 - 1, dialogbg )"

				// single pixel fills in the corners
				5="fill( x0 + 1, y0 + 1, x0 + 2, y0 + 2, ClientBG )"
				6="fill( x1 - 11, y0 + 1, x1 - 10, y0 + 2, ClientBG )"
				7="fill( x0 + 1, y1 - 2, x0 + 2, y1 - 1, ClientBG )"
				8="fill( x1 - 11, y1 - 2, x1 - 10, y1 - 1, ClientBG )"
				9="fill( x0, y0 + 1, x0 + 1, y0 + 2, ClientBG )"
				10="fill( x1 - 11, y0 + 1, x1 - 10, y0 + 2, ClientBG )"
				11="fill( x0, y1 - 2, x0 + 1, y1 - 1, ClientBG )"
				12="fill( x1 - 11, y1 - 2, x1 - 10, y1 - 1, ClientBG )"
				13="fill( x0 + 1, y0, x0 + 2, y0 + 1, ClientBG )" 
				14="fill( x1 - 11, y0, x1 - 10, y0 + 1, ClientBG )"
				15="fill( x0 + 1, y1 - 1, x0 + 2, y1, ClientBG )"
				16="fill( x1 - 11, y1 - 1, x1 - 10, y1, ClientBG )"
			}
			
		}
		
